<Window x:Class="WpfApplication1.OpenReport"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:WpfApplication1"
        xmlns:collections="clr-namespace:System.Collections;assembly=mscorlib"
        mc:Ignorable="d"
        Title="Калькулятор дезсредств | Все отчеты" Height="450" Width="300">
    <Grid>
        <ListBox x:Name="reportList" MouseDoubleClick="ReportList_OnMouseDoubleClick">
            <TextBlock FontWeight="Bold" TextDecorations="Underline" Text="Список всех сохраненных отчетов:" />
        </ListBox>
    </Grid>
</Window>
